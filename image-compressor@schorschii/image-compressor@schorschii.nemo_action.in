[Nemo Action]

_Name=Compress Image(s)
_Comment=Compresses the selected images and saves it with suffix \"_converted\" (e.g. for sending them via email)
Icon-Name=imagemagick

Exec=<image-compressor@schorschii/compress-images.py %U>

Mimetypes=image/*;
Selection=notnone

Dependencies=python3;

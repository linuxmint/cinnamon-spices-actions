#. metadata.json->name
#. install-fonts@anaximeno.nemo_action.in->Name
#. text.py:14
msgid "Install fonts"
msgstr "Cài đặt phông chữ"

#. text.py:16
msgid "Fonts were installed successfuly!"
msgstr "Cài đặt phông chữ thành công!"

#. text.py:18
msgid "Fonts could not be installed!"
msgstr "Không thể cài đặt phông chữ!"

#. text.py:20
msgid "Some fonts could not be installed!"
msgstr "Một số phông chữ không thể cài đặt!"

#. text.py:22
msgid "Updating fonts cache..."
msgstr "Đang cập nhật bộ nhớ đệm phông chữ..."

#. text.py:25
msgid ""
"Could not update fonts cache.\n"
"The installation of the fonts may not take immediate effect."
msgstr ""
"Không thể cập nhật bộ nhớ đệm phông chữ.\n"
"Việc cài đặt phông chữ có thể không có hiệu lực ngay lập tức."

#. text.py:29
msgid "Post installation issues"
msgstr "Vấn đề sau cài đặt"

#. text.py:32
msgid ""
"Some similar fonts are already installed.\n"
"What should be done in this case?"
msgstr ""
"Một số phông chữ tương tự đã được cài đặt.\n"
"Nên xử lý thế nào trong trường hợp này?"

#. text.py:35
msgid "Cancel"
msgstr "Hủy"

#. text.py:37
msgid "Ignore"
msgstr "Bỏ qua"

#. text.py:39
msgid "Override"
msgstr "Ghi đè"

#. metadata.json->description
#. install-fonts@anaximeno.nemo_action.in->Comment
msgid "Install selected fonts to your system account."
msgstr "Cài đặt các phông chữ được chọn vào tài khoản hệ thống của bạn."

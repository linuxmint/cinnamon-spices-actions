#. metadata.json->description
#. pdf-truncate-batch-print@goebbe.nemo_action.in->Comment
msgid "Cut PDF(s) pages at the start and from the end and batch-print them"
msgstr "Corte páginas PDF al principio y al final e imprímalas en lotes"

#. metadata.json->name
#. pdf-truncate-batch-print@goebbe.nemo_action.in->Name
msgid "Truncate and batch-print PDF(s)"
msgstr "Truncar e imprimir PDF por lotes"

#. pdf-truncate-batch-print.sh:28
msgid "Truncate PDF-printing"
msgstr "Truncar la impresión de PDF"

#. pdf-truncate-batch-print.sh:34
msgid ""
"Number of pages that should NOT be printed.\\n Empty fields result in no "
"truncation."
msgstr ""
"Número de páginas que NO deben imprimirse.\\n Los campos vacíos no producen "
"ningún truncamiento."

#. pdf-truncate-batch-print.sh:38
msgid "Number of pages cut at the start:"
msgstr "Número de páginas cortadas al principio:"

#. pdf-truncate-batch-print.sh:41
msgid "Number of pages cut from the end:"
msgstr "Número de páginas cortadas del final:"

#. pdf-truncate-batch-print.sh:69
msgid "Select a printer for batch-printing"
msgstr "Seleccionar una impresora para la impresión por lotes"

#. pdf-truncate-batch-print.sh:72
msgid "Printers:"
msgstr "Impresoras:"

#. pdf-truncate-batch-print.sh:94
#, sh-format
msgid ""
" <b>Summary:</b> \\n Number of pages cut at the start: $lefttrunc \\n Number "
"of pages cut from the end: $righttrunc \\n Printer: $myprinter \\n \\n "
"Choose one-sided or two-sided printing  \\n \\n <b>Batch-printing will start "
"instantly! </b>"
msgstr ""
" <b>Resumen:</b> \\n Número de páginas cortadas al principio: $lefttrunc \\n "
"Número de páginas cortadas desde el final: $righttrunc \\n Impresora: "
"$myprinter \\n \\n Elija entre impresión a una o dos caras \\n \\n <b>¡La "
"impresión por lotes comenzará instantáneamente! </b>"

#. pdf-truncate-batch-print.sh:102
msgid "one-sided"
msgstr "una cara"

#. pdf-truncate-batch-print.sh:105
msgid "Cancel"
msgstr "Cancelar"

#. pdf-truncate-batch-print.sh:108
msgid "two-sided"
msgstr "dos caras"

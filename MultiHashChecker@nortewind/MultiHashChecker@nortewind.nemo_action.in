[Nemo Action]
Active=true
_Name=Multi Hash Cheker
_Comment=Verifies the integrity of a file by comparing it to any of the most common hashes (md5, sha1, sha256, sha512).
Exec=<MultiHashChecker@nortewind/check_multihash.sh %F>
Icon-Name=auth-fingerprint-symbolic
Selection=S
Extensions=any;nodirs
Terminal=false
Dependencies=zenity;md5sum;sha1sum;sha256sum;sha512sum;

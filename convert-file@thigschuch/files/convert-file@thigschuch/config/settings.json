{
    "_WARNING_": "DO NOT EDIT THIS FILE! It will be overwritten on updates. To customize settings, edit user_settings.json in ~/.config/convert-file@thigschuch/",
    "version": "2.0",
    "directory_creation_threshold": 5,
    "output_directory_name": "converted_files",
    "temporary": {
        "directory": "/tmp",
        "directory_prefix": "convert_file_",
        "file_prefix": "convert_file_",
        "file_suffix": ".tmp"
    },
    "use_canonical_formats": true,
    "notifications": {
        "enabled": true,
        "on_start": false,
        "on_success": true,
        "on_failure": true,
        "on_cancel": true,
        "on_batch_start": false,
        "on_batch_finish": true,
        "on_batch_cancel": true,
        "on_batch_step_start": false,
        "on_batch_step_success": false,
        "on_batch_step_failure": true,
        "on_missing_dependency": true,
        "on_settings_corrupted": true
    },
    "format_aliases": {
        "ALAC": "M4A",
        "DOC": "DOCX",
        "HTM": "HTML",
        "JPG": "JPEG",
        "MK3D": "MKV",
        "MPG": "MPEG",
        "M4V": "MP4",
        "PPT": "PPTX",
        "TIF": "TIFF",
        "XLS": "XLSX",
        "YML": "YAML"
    },
    "preselect_format_by_usage": true,
    "default_formats": {
        "IMAGE": [
            "PNG",
            "JPEG"
        ],
        "VIDEO": [
            "MP4",
            "MKV"
        ],
        "AUDIO": [
            "MP3",
            "AAC"
        ],
        "DOCUMENT": [
            "PDF",
            "DOCX"
        ],
        "OFFICE": [
            "PDF",
            "DOCX"
        ],
        "SPREADSHEET": [
            "XLSX",
            "CSV"
        ],
        "PRESENTATION": [
            "PPTX",
            "PDF"
        ],
        "MARKUP": [
            "HTML",
            "MD"
        ],
        "DATA": [
            "JSON",
            "TXT"
        ],
        "ARCHIVE": [
            "ZIP",
            "ISO"
        ]
    },
    "output_restricted_formats": [
        "ARW",
        "CR2",
        "DEB",
        "DMG",
        "DNG",
        "NEF",
        "ORF",
        "PEF",
        "RAF",
        "RAW",
        "RPM",
        "RW2",
        "SR2"
    ],
    "conversion_exclusions": [],
    "archive_rules": {
        "by_target": {
            "7Z": [
                "7z x '{input}' -o'{temp_dir}/' -bb0",
                "cd '{temp_dir}'",
                "7z a -t7z -mx=9 '{output}' ."
            ],
            "ISO": [
                "7z x '{input}' -o'{temp_dir}/' -bb0",
                "genisoimage -o '{output}' -R -J -joliet-long '{temp_dir}'"
            ],
            "RAR": [
                "7z x '{input}' -o'{temp_dir}/' -bb0",
                "cd '{temp_dir}'",
                "rar a -m5 '{output}' ."
            ],
            "TAR": [
                "7z x '{input}' -o'{temp_dir}/' -bb0",
                "cd '{temp_dir}'",
                "tar -cf '{output}' ."
            ],
            "TAR.BZ2": [
                "7z x '{input}' -o'{temp_dir}/' -bb0",
                "cd '{temp_dir}'",
                "tar -cjf '{output}' ."
            ],
            "TAR.GZ": [
                "7z x '{input}' -o'{temp_dir}/' -bb0",
                "cd '{temp_dir}'",
                "tar -czf '{output}' ."
            ],
            "TAR.LZMA": [
                "7z x '{input}' -o'{temp_dir}/' -bb0",
                "cd '{temp_dir}'",
                "tar -cf - . | lzma -c > '{output}'"
            ],
            "TAR.LZO": [
                "7z x '{input}' -o'{temp_dir}/' -bb0",
                "cd '{temp_dir}'",
                "tar -cf - . | lzop -c > '{output}'"
            ],
            "TAR.XZ": [
                "7z x '{input}' -o'{temp_dir}/' -bb0",
                "cd '{temp_dir}'",
                "tar -cJf '{output}' ."
            ],
            "TGZ": [
                "7z x '{input}' -o'{temp_dir}/' -bb0",
                "cd '{temp_dir}'",
                "tar -czf '{output}' ."
            ],
            "ZIP": [
                "7z x '{input}' -o'{temp_dir}/' -bb0",
                "cd '{temp_dir}'",
                "zip -r -9 '{output}' ."
            ]
        },
        "default": [
            "7z x '{input}' -o'{temp_dir}/' -bb0",
            "cd '{temp_dir}'",
            "7z a -mx=9 '{output}' ."
        ]
    },
    "audio_rules": {
        "by_target": {
            "AAC": "ffmpeg -i '{input}' -map 0:a -map_metadata 0 -codec:a aac -b:a 256k -ar 48000 '{output}'",
            "FLAC": "ffmpeg -i '{input}' -map 0:a -map_metadata 0 -codec:a flac -compression_level 8 '{output}'",
            "M4A": "ffmpeg -i '{input}' -map 0:a -map_metadata 0 -codec:a alac '{output}'",
            "MP3": "ffmpeg -i '{input}' -map 0:a -map_metadata 0 -codec:a libmp3lame -q:a 0 -id3v2_version 3 '{output}'",
            "OGG": "ffmpeg -i '{input}' -map 0:a -map_metadata 0 -codec:a libvorbis -q:a 8 -ar 48000 '{output}'",
            "OPUS": "ffmpeg -i '{input}' -map 0:a -map_metadata 0 -codec:a libopus -b:a 128k '{output}'",
            "WAV": "ffmpeg -i '{input}' -map 0:a -codec:a pcm_s16le -ar 44100 '{output}'"
        },
        "default": "ffmpeg -i '{input}' -map 0:a -map_metadata 0 -q:a 0 '{output}'"
    },
    "data_rules": {
        "by_target": {
            "JSON": "pandoc '{input}' -o '{output}' -t json",
            "TXT": "pandoc '{input}' -o '{output}' -t plain",
            "YAML": "pandoc '{input}' -o '{output}' -t markdown"
        },
        "default": "pandoc '{input}' -o '{output}'"
    },
    "image_rules": {
        "by_target": {
            "AVIF": "convert '{input}' -auto-orient -quality 90 -define heic:speed=4 '{output}'",
            "GIF": "convert '{input}' -auto-orient -colors 256 '{output}'",
            "ICO": "convert '{input}' -auto-orient -define icon:auto-resize=256,128,64,48,32,16 '{output}'",
            "JPEG": "convert '{input}' -auto-orient -quality 90 -sampling-factor 4:2:0 '{output}'",
            "PNG": "convert '{input}' -auto-orient -define png:compression-level=9 '{output}'",
            "TIFF": "convert '{input}' -auto-orient -compress lzw '{output}'",
            "WEBP": "convert '{input}' -auto-orient -quality 90 -define webp:method=6 '{output}'"
        },
        "default": "convert '{input}' -auto-orient '{output}'"
    },
    "markup_rules": {
        "by_target": {
            "HTML": "pandoc '{input}' -o '{output}' --standalone --self-contained --metadata title='Document'",
            "MD": "pandoc '{input}' -o '{output}' -t markdown --wrap=auto",
            "XML": "pandoc '{input}' -o '{output}' -t docbook"
        },
        "default": "pandoc '{input}' -o '{output}'"
    },
    "office_rules": {
        "by_target": {
            "DOCX": [
                "libreoffice --headless --convert-to docx --outdir '{temp_dir}' '{input}'",
                "mv '{temp_dir}/{input_stem}.docx' '{output}'"
            ],
            "EPUB": "ebook-convert '{input}' '{output}' --enable-heuristics",
            "MOBI": "ebook-convert '{input}' '{output}' --enable-heuristics",
            "ODT": [
                "libreoffice --headless --convert-to odt --outdir '{temp_dir}' '{input}'",
                "mv '{temp_dir}/{input_stem}.odt' '{output}'"
            ],
            "PDF": [
                "libreoffice --headless --convert-to pdf --outdir '{temp_dir}' '{input}'",
                "mv '{temp_dir}/{input_stem}.pdf' '{output}'"
            ],
            "RTF": [
                "libreoffice --headless --convert-to rtf --outdir '{temp_dir}' '{input}'",
                "mv '{temp_dir}/{input_stem}.rtf' '{output}'"
            ]
        },
        "default": [
            "libreoffice --headless --convert-to {format} --outdir '{temp_dir}' '{input}'",
            "mv '{temp_dir}/{input_stem}.{format}' '{output}'"
        ]
    },
    "presentation_rules": {
        "by_target": {
            "ODP": [
                "libreoffice --headless --convert-to odp --outdir '{temp_dir}' '{input}'",
                "mv '{temp_dir}/{input_stem}.odp' '{output}'"
            ],
            "PPTX": [
                "libreoffice --headless --convert-to pptx --outdir '{temp_dir}' '{input}'",
                "mv '{temp_dir}/{input_stem}.pptx' '{output}'"
            ]
        },
        "default": [
            "libreoffice --headless --convert-to {format} --outdir '{temp_dir}' '{input}'",
            "mv '{temp_dir}/{input_stem}.{format}' '{output}'"
        ]
    },
    "spreadsheet_rules": {
        "by_target": {
            "CSV": [
                "libreoffice --headless --convert-to csv --outdir '{temp_dir}' '{input}'",
                "mv '{temp_dir}/{input_stem}.csv' '{output}'"
            ],
            "ODS": [
                "libreoffice --headless --convert-to ods --outdir '{temp_dir}' '{input}'",
                "mv '{temp_dir}/{input_stem}.ods' '{output}'"
            ],
            "XLSX": [
                "libreoffice --headless --convert-to xlsx --outdir '{temp_dir}' '{input}'",
                "mv '{temp_dir}/{input_stem}.xlsx' '{output}'"
            ]
        },
        "default": [
            "libreoffice --headless --convert-to {format} --outdir '{temp_dir}' '{input}'",
            "mv '{temp_dir}/{input_stem}.{format}' '{output}'"
        ]
    },
    "video_rules": {
        "by_target": {
            "AVI": "ffmpeg -i '{input}' -codec:v mpeg4 -q:v 3 -codec:a libmp3lame -b:a 192k '{output}'",
            "FLV": "ffmpeg -i '{input}' -codec:v libx264 -crf 23 -codec:a aac -b:a 128k '{output}'",
            "MKV": "ffmpeg -i '{input}' -codec:v libx265 -crf 23 -preset medium -codec:a aac -b:a 192k '{output}'",
            "MOV": "ffmpeg -i '{input}' -codec:v libx264 -crf 20 -preset medium -codec:a aac -b:a 192k -movflags +faststart '{output}'",
            "MPEG": "ffmpeg -i '{input}' -codec:v mpeg2video -q:v 2 -codec:a mp2 -b:a 192k '{output}'",
            "MP4": "ffmpeg -i '{input}' -codec:v libx264 -crf 20 -preset medium -codec:a aac -b:a 192k -movflags +faststart '{output}'",
            "WEBM": "ffmpeg -i '{input}' -codec:v libvpx-vp9 -crf 24 -b:v 0 -codec:a libopus -b:a 128k '{output}'"
        },
        "default": "ffmpeg -i '{input}' -codec:v libx264 -crf 23 -preset medium -codec:a aac -b:a 192k '{output}'"
    },
    "special_rules": [
        {
            "_COMMENT_": "ARCHIVE"
        },
        {
            "from": "DEB",
            "to": "7Z",
            "command": [
                "ar x '{input}' --output='{temp_dir}'",
                "cd '{temp_dir}'",
                "7z a -t7z -mx=9 '{output}' ."
            ]
        },
        {
            "from": "DEB",
            "to": "ISO",
            "command": [
                "ar x '{input}' --output='{temp_dir}'",
                "genisoimage -o '{output}' -R -J -joliet-long '{temp_dir}'"
            ]
        },
        {
            "from": "DEB",
            "to": "RAR",
            "command": [
                "ar x '{input}' --output='{temp_dir}'",

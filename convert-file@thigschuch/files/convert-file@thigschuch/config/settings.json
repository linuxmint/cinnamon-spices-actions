        },
        {
            "from": "MKV",
            "to": "GIF",
            "command": "ffmpeg -i '{input}' -vf 'fps=10,scale=480:-1:flags=lanczos,split[s0][s1];[s0]palettegen=stats_mode=diff[p];[s1][p]paletteuse=dither=bayer:bayer_scale=5' '{output}'"
        },
        {
            "from": "MOV",
            "to": "GIF",
            "command": "ffmpeg -i '{input}' -vf 'fps=10,scale=480:-1:flags=lanczos,split[s0][s1];[s0]palettegen=stats_mode=diff[p];[s1][p]paletteuse=dither=bayer:bayer_scale=5' '{output}'"
        },
        {
            "from": "MP4",
            "to": "GIF",
            "command": "ffmpeg -i '{input}' -vf 'fps=10,scale=480:-1:flags=lanczos,split[s0][s1];[s0]palettegen=stats_mode=diff[p];[s1][p]paletteuse=dither=bayer:bayer_scale=5' '{output}'"
        },
        {
            "from": "WEBM",
            "to": "GIF",
            "command": "ffmpeg -i '{input}' -vf 'fps=10,scale=480:-1:flags=lanczos,split[s0][s1];[s0]palettegen=stats_mode=diff[p];[s1][p]paletteuse=dither=bayer:bayer_scale=5' '{output}'"
        },
        {
            "_COMMENT_": "VIDEO TO AUDIO"
        },
        {
            "from": "AVI",
            "to": "MP3",
            "command": "ffmpeg -i '{input}' -vn -codec:a libmp3lame -q:a 0 '{output}'"
        },
        {
            "from": "MKV",
            "to": "MP3",
            "command": "ffmpeg -i '{input}' -vn -codec:a libmp3lame -q:a 0 '{output}'"
        },
        {
            "from": "MOV",
            "to": "MP3",
            "command": "ffmpeg -i '{input}' -vn -codec:a libmp3lame -q:a 0 '{output}'"
        },
        {

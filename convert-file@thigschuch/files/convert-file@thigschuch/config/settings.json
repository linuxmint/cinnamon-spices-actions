            "command": [
                "rpm2cpio '{input}' | cpio -idmv -D '{temp_dir}'",
                "genisoimage -o '{output}' -R -J -joliet-long '{temp_dir}'"
            ]
        },
        {
            "from": "RPM",
            "to": "RAR",
            "command": [
                "rpm2cpio '{input}' | cpio -idmv -D '{temp_dir}'",
                "cd '{temp_dir}'",
                "rar a -m5 '{output}' ."
            ]
        },
        {
            "from": "RPM",
            "to": "TAR",
            "command": [
                "rpm2cpio '{input}' | cpio -idmv -D '{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -cf '{output}' ."
            ]
        },
        {
            "from": "RPM",
            "to": "TAR.BZ2",
            "command": [
                "rpm2cpio '{input}' | cpio -idmv -D '{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -cjf '{output}' ."
            ]
        },
        {
            "from": "RPM",
            "to": "TAR.GZ",
            "command": [
                "rpm2cpio '{input}' | cpio -idmv -D '{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -czf '{output}' ."
            ]
        },
        {
            "from": "RPM",
            "to": "TAR.LZMA",
            "command": [
                "rpm2cpio '{input}' | cpio -idmv -D '{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -cf - . | lzma -c > '{output}'"
            ]
        },
        {
            "from": "RPM",
            "to": "TAR.LZO",
            "command": [
                "rpm2cpio '{input}' | cpio -idmv -D '{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -cf - . | lzop -c > '{output}'"
            ]
        },
        {
            "from": "RPM",
            "to": "TAR.XZ",
            "command": [
                "rpm2cpio '{input}' | cpio -idmv -D '{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -cJf '{output}' ."
            ]
        },
        {
            "from": "RPM",
            "to": "TGZ",
            "command": [
                "rpm2cpio '{input}' | cpio -idmv -D '{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -czf '{output}' ."
            ]
        },
        {
            "from": "RPM",
            "to": "ZIP",
            "command": [
                "rpm2cpio '{input}' | cpio -idmv -D '{temp_dir}'",
                "cd '{temp_dir}'",
                "zip -r -9 '{output}' ."
            ]
        },
        {
            "_COMMENT_": "AUDIO"
        },
        {
            "_COMMENT_": "DATA"
        },
        {
            "from": "JSON",
            "to": "TXT",
            "command": "python3 -c \"import json; print(json.dumps(json.load(open('{input}')), indent=2), file=open('{output}', 'w'))\""
        },
        {
            "from": "JSON",
            "to": "XML",
            "command": "python3 -c \"import json, xml.etree.ElementTree as ET; data=json.load(open('{input}')); root=ET.Element('root'); ET.SubElement(root, 'data').text=str(data); ET.ElementTree(root).write('{output}', encoding='unicode')\""
        },
        {
            "from": "JSON",
            "to": "YAML",
            "command": "python3 -c \"import json, yaml, sys; yaml.dump(json.load(open('{input}')), open('{output}', 'w'), default_flow_style=False)\""
        },
        {
            "from": "YAML",
            "to": "JSON",
            "command": "python3 -c \"import json, yaml; json.dump(yaml.safe_load(open('{input}')), open('{output}', 'w'), indent=2)\""
        },
        {
            "from": "YAML",
            "to": "TXT",
            "command": "python3 -c \"import yaml; print(yaml.dump(yaml.safe_load(open('{input}')), default_flow_style=False), file=open('{output}', 'w'))\""
        },
        {
            "from": "YAML",
            "to": "XML",
            "command": "python3 -c \"import yaml, xml.etree.ElementTree as ET; data=yaml.safe_load(open('{input}')); root=ET.Element('root'); ET.SubElement(root, 'data').text=str(data); ET.ElementTree(root).write('{output}', encoding='unicode')\""
        },
        {
            "_COMMENT_": "IMAGE"
        },
        {
            "from": "SVG",
            "to": "JPEG",
            "command": "convert -background white -density 300 '{input}' -quality 90 '{output}'"
        },
        {
            "from": "SVG",
            "to": "PNG",
            "command": "convert -background none -density 300 '{input}' '{output}'"
        },
        {
            "from": "WEBP",
            "to": "GIF",
            "command": "convert '{input}' -coalesce '{output}'"
        },
        {
            "_COMMENT_": "MARKUP"
        },
        {
            "_COMMENT_": "OFFICE"
        },
        {
            "from": "EPUB",
            "to": "DOCX",
            "command": "pandoc '{input}' -o '{output}'"
        },
        {
            "from": "EPUB",
            "to": "PDF",
            "command": "ebook-convert '{input}' '{output}' --enable-heuristics --paper-size=a4"
        },
        {
            "from": "EPUB",
            "to": "RTF",
            "command": "pandoc '{input}' -o '{output}'"
        },
        {
            "from": "MOBI",
            "to": "DOCX",
            "command": [
                "ebook-convert '{input}' '{temp_file}.epub' --enable-heuristics",
                "pandoc '{temp_file}.epub' -o '{output}'"
            ],
            "temp_file_suffix": ".epub"
        },
        {
            "from": "MOBI",
            "to": "ODT",
            "command": [
                "ebook-convert '{input}' '{temp_file}.epub' --enable-heuristics",
                "pandoc '{temp_file}.epub' -o '{output}'"
            ],
            "temp_file_suffix": ".epub"
        },
        {
            "from": "MOBI",
            "to": "PDF",
            "command": "ebook-convert '{input}' '{output}' --enable-heuristics --paper-size=a4"
        },
        {
            "from": "MOBI",
            "to": "RTF",
            "command": [
                "ebook-convert '{input}' '{temp_file}' --enable-heuristics",
                "pandoc '{temp_file}' -o '{output}'"
            ]
        },
        {
            "_COMMENT_": "PRESENTATION"
        },
        {
            "_COMMENT_": "SPREADSHEET"
        },
        {
            "_COMMENT_": "VIDEO"
        },
        {
            "_COMMENT_": "IMAGE TO VIDEO"
        },
        {
            "from": "GIF",
            "to": "MP4",
            "command": "ffmpeg -i '{input}' -movflags faststart -pix_fmt yuv420p -vf 'scale=trunc(iw/2)*2:trunc(ih/2)*2' '{output}'"
        },
        {
            "from": "GIF",
            "to": "WEBM",
            "command": "ffmpeg -i '{input}' -c:v libvpx-vp9 -b:v 0 -crf 30 -vf 'scale=trunc(iw/2)*2:trunc(ih/2)*2' '{output}'"
        },
        {
            "_COMMENT_": "IMAGE TO OFFICE"
        },
        {
            "from": "JPEG",
            "to": "PDF",
            "command": "convert '{input}' '{output}'"
        },
        {
            "from": "PNG",
            "to": "PDF",
            "command": "convert '{input}' '{output}'"
        },
        {

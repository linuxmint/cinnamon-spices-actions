                "rar a -m5 '{output}' ."
            ]
        },
        {
            "from": "DEB",
            "to": "TAR",
            "command": [
                "ar x '{input}' --output='{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -cf '{output}' ."
            ]
        },
        {
            "from": "DEB",
            "to": "TAR.BZ2",
            "command": [
                "ar x '{input}' --output='{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -cjf '{output}' ."
            ]
        },
        {
            "from": "DEB",
            "to": "TAR.GZ",
            "command": [
                "ar x '{input}' --output='{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -czf '{output}' ."
            ]
        },
        {
            "from": "DEB",
            "to": "TAR.LZMA",
            "command": [
                "ar x '{input}' --output='{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -cf - . | lzma -c > '{output}'"
            ]
        },
        {
            "from": "DEB",
            "to": "TAR.LZO",
            "command": [
                "ar x '{input}' --output='{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -cf - . | lzop -c > '{output}'"
            ]
        },
        {
            "from": "DEB",
            "to": "TAR.XZ",
            "command": [
                "ar x '{input}' --output='{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -cJf '{output}' ."
            ]
        },
        {
            "from": "DEB",
            "to": "TGZ",
            "command": [
                "ar x '{input}' --output='{temp_dir}'",
                "cd '{temp_dir}'",
                "tar -czf '{output}' ."
            ]
        },
        {
            "from": "DEB",

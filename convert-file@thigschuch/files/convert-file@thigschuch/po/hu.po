#: text.py:14 text.py:26
msgid "Error"
msgstr "Hiba"

#: text.py:15
msgid "The file format is not supported."
msgstr "A fájlformátum nem támogatott."

#: text.py:17
msgid "Select a format"
msgstr "Válasszon formátumot"

#: text.py:18
msgid "Select the format to convert the file to."

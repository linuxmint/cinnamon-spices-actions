#. ui/aui.py:164 utils/text.py:63
msgid "OK"
msgstr "Ados"

#. utils/text.py:57
msgid "File Converter"
msgstr "Fitxategi bihurtzailea"

#. utils/text.py:58
msgid "Choose output format:"
msgstr "Aukeratu irteera formatua:"

#. utils/text.py:59
#, python-brace-format
msgid ""
"Converting\n"
"{file} to {extension}"
msgstr "Bihurtzen\n{file} {extension} formatura"


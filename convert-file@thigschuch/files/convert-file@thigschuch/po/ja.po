#. text.py:14 text.py:26
msgid "Error"
msgstr "エラー"

#. text.py:15
msgid "The file format is not supported."
msgstr "このファイル形式はサポートされていません。"

#. text.py:17
msgid "Select a format"
msgstr "形式を選択"

#. text.py:18
msgid "Select the format to convert the file to."
msgstr "ファイルをどの形式に変換するか選択してください。"

#. text.py:20
msgid "Converting"
msgstr "変換しています"

#. text.py:21
#, python-brace-format
msgid "Converting file to {format}..."
msgstr "ファイルを {format} に変換しています..."

#. text.py:23
msgid "Success"
msgstr "成功"

#. text.py:24
#, python-brace-format
msgid "The file has been converted to {format}."
msgstr "ファイルが {format} に変換されました。"

#. text.py:27
msgid "An error occurred while converting the file."
msgstr "ファイルの変換中にエラーが発生しました。"

#. text.py:29
msgid "Invalid file"
msgstr "無効なファイル"

#. text.py:30
msgid "The file is not valid."
msgstr "ファイルが有効ではありません。"

#. text.py:32
msgid "No file name or extension"
msgstr "ファイル名または拡張子なし"

#. text.py:33
msgid "The file must have a name and an extension."
msgstr "ファイルには名前と拡張子が必要です。"


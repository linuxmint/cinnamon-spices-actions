#, python-brace-format
msgid ""
"Invalid file:\n"
"{path}\n"
"\n"
"Select a file, not a folder."
msgstr ""
"Ógild skrá:\n"
"{path}\n"
"\n"
"Veldu skrá, ekki möppu."

#. utils/text.py:85
#, python-brace-format
msgid ""
"No file extension:\n"
"{path}\n"
"\n"
"Add an extension (e.g., .jpg, .mp4, .pdf) to determine the format."
msgstr ""
"Engin skráarending:\n"
"{path}\n"
"\n"
"Bættu við endingunni (t.d. .jpg, .mp4, .pdf) til að ákvarða formið."


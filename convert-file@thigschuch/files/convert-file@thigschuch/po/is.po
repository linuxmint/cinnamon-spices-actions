#. utils/text.py:79
#, python-brace-format
msgid ""
"File not found:\n"
"{path}\n"
"\n"
"Check if the file exists."
msgstr ""
"Skrá fannst ekki:\n"
"{path}\n"
"\n"
"Athugaðu hvort skráin sé til."


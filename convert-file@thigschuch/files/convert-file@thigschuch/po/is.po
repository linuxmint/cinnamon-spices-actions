#. text.py:14 text.py:26
msgid "Error"
msgstr "Villa"

#. text.py:15
msgid "The file format is not supported."
msgstr "Skráartegundin er ekki studd."

#. text.py:17
msgid "Select a format"
msgstr "Veldu snið"

#. text.py:18
msgid "Select the format to convert the file to."
msgstr "Veldu sniðið sem umbreyta á skránni yfir í."

#. text.py:20
msgid "Converting"
msgstr "Umbreyti"

#. text.py:21
#, python-brace-format
msgid "Converting file to {format}..."
msgstr "Umbreyti skrá yfir í {format}..."

#. text.py:23
msgid "Success"
msgstr "Tókst"

#. text.py:24
#, python-brace-format
msgid "The file has been converted to {format}."
msgstr "Skránni hefur verið umbreytt yfir í {format}."

#. text.py:27
msgid "An error occurred while converting the file."
msgstr "Villa kom upp við að umbreyta skránni."

#. text.py:29
msgid "Invalid file"
msgstr "Ógild skrá"

#. text.py:30
msgid "The file is not valid."
msgstr "Skráin er ekki gild."

#. text.py:32
msgid "No file name or extension"
msgstr "Ekkert skráarheiti eða skráarending"

#. text.py:33
msgid "The file must have a name and an extension."
msgstr "Skráin verður að hafa skráarheiti og skráarendingu."

#. metadata.json->description
#. convert-file@thigschuch.nemo_action.in->Comment
msgid "Converts a file to a different format"
msgstr "Umbreytir skrá yfir á annað snið"

#. metadata.json->name
#. convert-file@thigschuch.nemo_action.in->Name
msgid "Convert File"
msgstr "Umbreyta skrá"

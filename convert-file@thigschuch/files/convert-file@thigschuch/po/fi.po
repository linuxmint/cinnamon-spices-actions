#: text.py:14 text.py:26
msgid "Error"
msgstr "Virhe"

#: text.py:15
msgid "The file format is not supported."
msgstr "Formaattia ei tueta."

#: text.py:17
msgid "Select a format"
msgstr "Valitse formaatti"

#: text.py:18
msgid "Select the format to convert the file to."
msgstr "Valitse formaatti, johon tiedosto muunnetaan."

#: text.py:20
msgid "Converting"
msgstr "Muunnetaan"

#: text.py:21
#, python-brace-format
msgid "Converting file to {format}..."
msgstr "Muunnetaan tiedosto {format}..."

#: text.py:23
msgid "Success"
msgstr "Onnistui"

#: text.py:24
#, python-brace-format
msgid "The file has been converted to {format}."
msgstr "Tiedosto on muunnettu {format}."

#: text.py:27
msgid "An error occurred while converting the file."
msgstr "Tiedostoa muunnettaessa tapahtui virhe."

#: text.py:29
msgid "Invalid file"
msgstr "Viallinen tiedosto"

#: text.py:30
msgid "The file is not valid."
msgstr "Tiedosto ei ole kelvollinen."

#: text.py:32
msgid "No file name or extension"
msgstr "Ei tiedostonimeä tai päätettä"

#: text.py:33
msgid "The file must have a name and an extension."
msgstr "Tiedostolla on oltava nimi ja tiedostopääte."


#. metadata.json->description
msgid "Verify the sha256sum for a single file"
msgstr "Überprüft die SHA256-Prüfsumme einer Datei"

#. metadata.json->name
#. Name
msgid "Verify sha256sum"
msgstr "SHA256-Prüfsumme überprüfen"

#. Comment
msgid "Provide a sha256sum hash to verify with this file"
msgstr "Geben Sie eine SHA256-Prüfsumme ein, um sie mit der Datei zu vergleichen"

#: ../verify-sha256sum@rcalixte.sh:8
msgid "Calculating sha256sum for"
msgstr "Berechne SHA256-Prüfsumme für"

#: ../verify-sha256sum@rcalixte.sh:9
msgid "Please enter the hash (64 characters) to match:"
msgstr "Bitte geben Sie den Hashwert (64 Zeichen) zur Überprüfung ein:"

#: ../verify-sha256sum@rcalixte.sh:10
msgid "The provided hash was invalid. Please try again."
msgstr "Der eingegebene Hashwert stimmt nicht überein. Bitte versuchen Sie es erneut."

#: ../verify-sha256sum@rcalixte.sh:11
msgid "The hashes are the same"
msgstr "Die Hashwerte stimmen überein"

#: ../verify-sha256sum@rcalixte.sh:12
msgid "The hashes are not the same"
msgstr "Die Hashwerte stimmen nicht überein"

#: ../verify-sha256sum@rcalixte.sh:13
msgid "The hash you provided"
msgstr "Der eingebene Hashwert"

#: ../verify-sha256sum@rcalixte.sh:14
msgid "The file's hash"
msgstr "Der Datei-Hashwert"

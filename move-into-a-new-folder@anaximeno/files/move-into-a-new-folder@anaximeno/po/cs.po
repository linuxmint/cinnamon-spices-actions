#. metadata.json->name
#: text.py:13
msgid "Move Into a New Folder"
msgstr "Přesunout do nové složky"

#: text.py:15
msgid "Name of the new folder:"
msgstr "Název nové složky:"

#: text.py:17
msgid "New Folder"
msgstr "Nová složka"

#: text.py:20
#, python-format
msgid ""
"Folder '%s' already exists inside the current directory, do you want to move "
"the selected files inside?"
msgstr ""
"V aktuálním adresáři již existuje složka '%s', chcete vybrané soubory "
"přesunout dovnitř?"

#: text.py:25
#, python-format
msgid "Couldn't create a new folder '%s' inside the current directory!"
msgstr "Nepodařilo se vytvořit novou složku '%s' uvnitř aktuálního adresáře!"

#: text.py:28
#, python-format
msgid "Could not move %d of the selected items into a new folder!"
msgstr "Nepodařilo se přesunout %d vybrané položky do nové složky!"

#: text.py:30
msgid "Could not move any of the selected items into a new folder!"
msgstr "Nepodařilo se přesunout žádnou z vybraných položek do nové složky!"

#. metadata.json->description
#. move-into-a-new-folder@anaximeno.nemo_action.in->Comment
msgid "Move selected files, folders, and other items inside a new directory"
msgstr "Přesuňte vybrané soubory, složky a dalších položky do nového adresáře"

#. move-into-a-new-folder@anaximeno.nemo_action.in->Name
msgid "Move into a new folder"
msgstr "Přesunout do nové složky"

#. metadata.json->name
#: action.py:17
msgid "Move Into a New Folder"
msgstr "Flytta till en ny mapp"

#: action.py:18
msgid "Name of the new folder:"
msgstr "Namn på den nya mappen:"

#: action.py:19
msgid "New Folder"
msgstr "Ny mapp"

#: action.py:21
#, python-format
msgid ""
"Folder '%s' already exists inside the current directory, do you want to move "
"the selected files inside?"
msgstr ""
"Mappen \"%s\" finns redan i den aktuella katalogen, vill du flytta de "
"markerade filerna till den?"

#: action.py:24
#, python-format
msgid "Couldn't create a new folder '%s' inside the current directory!"
msgstr "Kunde inte skapa en ny mapp \"%s\" i den aktuella katalogen!"

#: action.py:26
#, python-format
msgid "Could not move %d of the selected items into a new folder!"
msgstr "Kunde inte flytta %d av de markerade objekten till en ny mapp!"

#: action.py:27
msgid "Could not move any of the selected items into a new folder!"
msgstr "Kunde inte flytta något av de markerade objekten till en ny mapp!"

#. metadata.json->description
#. Comment
msgid "Move selected files, folders, and other items inside a new directory"
msgstr "Flytta markerade filer, mappar och andra objekt till en ny katalog"

#. Name
msgid "Move into a new folder"
msgstr "Flytta till en ny mapp"

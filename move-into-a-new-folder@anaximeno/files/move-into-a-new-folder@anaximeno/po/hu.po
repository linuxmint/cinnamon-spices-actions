#. metadata.json->description
#. Comment
msgid "Move selected files, folders, and other items inside a new directory"
msgstr "Kijelölt fájlok, mappák és egyéb elemek áthelyezése egy új könyvtárba"

#. metadata.json->name
msgid "Move Into a New Folder"
msgstr "Áthelyezés új Mappába"

#. Name
msgid "Move into a new folder"
msgstr "Áthelyezés új mappába"

#. action.py:18
msgid "Name of the new folder:"
msgstr "Az új mappa neve:"

#. action.py:19
msgid "New Folder"
msgstr "Új Mappa"

#. action.py: 21
msgid ""
"Folder '%s' already exists inside the current directory, do you want to move "
"the selected files inside?"
msgstr ""
"A '%s' mappa már létezik az aktuális könyvtárban, át akarja abba helyezni a "
"kiválasztott fájlokat?"

#. action.py:24
msgid "Couldn't create a folder '%s' inside the current directory!"
msgstr "Nem sikerült létrehozni a '%s' mappát az aktuális könyvtáron belül!"

#. action.py:26
msgid "Could not move %d of the selected items into a new folder!"
msgstr "Nem sikerült %d kiválasztott elemet áthelyezni egy új mappába!"

#. action.py:27
msgid "Could not move any of the selected items into a new folder!"
msgstr ""
"Nem sikerült a kiválasztott elemek egyikét sem áthelyezni egy új mappába!"
